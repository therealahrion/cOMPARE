<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearanceMedium">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/text_color</item>
    </style>
    <style name="TextAppearanceMediumBlue">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/text_color_bluehide</item>
    </style>
    <style name="TextAppearanceLargeBlue">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/text_color_bluehide</item>
    </style>
    <style name="TextAppearanceLarge">
        <item name="android:textSize">34.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/text_color</item>
    </style>
    <style name="TextAppearanceCopyright">
        <item name="android:textSize">11.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff666666</item>
    </style>
    <style name="SwitchButton">
        <item name="android:background">@drawable/switch_button</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textOn">" "</item>
        <item name="android:textOff">" "</item>
    </style>
    <style name="EqualizerCustomButton">
        <item name="android:background">@drawable/eq_custom_bkg</item>
        <item name="android:src">@drawable/eq_custom_image</item>
        <item name="android:scaleType">center</item>
    </style>
    <style name="SwitchButton_ldrtl">
        <item name="android:background">@drawable/switch_button_ldrtl</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textOn">" "</item>
        <item name="android:textOff">" "</item>
    </style>
</resources>
